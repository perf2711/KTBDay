@{
    ViewData["Title"] = "Komputronik Biznes Day";
}

<form>
    <div id="messages" style="background:rgb(190, 190, 190); height:400px; overflow:auto">

    </div>
    <div class="form-group">
        <label for="message">Wiadomość</label>
        <input type="text" class="form-control" id="message" placeholder="Wiadomość">
    </div>
    
    <div class="form-group">
        <label for="username">Nazwa użytkownika</label>
        <input type="text" class="form-control" id="username" placeholder="Nazwa użytkownika">
    </div>
    
    <button type="button" class="btn btn-primary" id="join" onclick="startSignalR()">Dołącz</button>
    <button type="button" class="btn btn-primary" onclick="sendMessage()">Wyślij</button>
</form>